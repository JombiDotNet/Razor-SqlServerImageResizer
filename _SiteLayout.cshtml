<!doctype html>
<html>
<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <title>@Page.Title</title>
  <meta name="description" content="@Page.Description" />

  <!-- Latest compiled and minified CSS -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" />

  <!-- Optional theme -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css" />

  <link type="text/plain" rel="author" href="~/humans.txt" />
  <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
</head>
<body>
  @RenderBody()

  @* JavaScript at the bottom for faster page loading *@
  @* Grab jQuery, with a protocol relative URL; fall back to local if offline *@
  @if (Request.Browser.Browser == "IE" && Request.Browser.Version.AsDecimal() < 9)
  {
    <text>
      <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.9.0.min.js"></script>
    </text>
  }
  else
  {
    <text>
      <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-2.1.4.min.js"></script>
      <script>window.jQuery || document.write('<script src="@Href("~/scripts/jquery-2.1.4.min.js")"><\/script>')</script>
    </text>
  }
  <!-- Latest compiled and minified JavaScript -->
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
</body>
</html>

